{
  "name": "tiny-markdown-server",
  "version": "2.0.0-dev",
  "description": "Displays markdown files in the browser.",
  "scripts": {
    "license:summary": "license-checker --production --summary --unknown",
    "license:generate": "node build_scripts/license.js",
    "test": "mocha test/*",
    "test:coverage": "istanbul cover --print both _mocha -- test/*",
    "app": "electron --trace-warnings main.js",
    "build": "npm run build:mac && npm run build:win",
    "build:mac": "node build_scripts/build-mac.js",
    "build:win": "node build_scripts/build-win.js"
  },
  "dependencies": {
    "bootstrap": "^4.6.1",
    "ejs": "^3.1.7",
    "ejs-electron": "^2.1.1",
    "escape-string-regexp": "^4.0.0",
    "express": "^4.17.3",
    "github-markdown-css": "^4.0.0",
    "jquery": "^3.6.0",
    "jsonschema": "^1.4.0",
    "markdown-it": "^12.3.2",
    "n-readlines": "^1.0.1",
    "open": "^8.4.0",
    "popper.js": "^1.16.1",
    "sprintf-js": "^1.1.2",
    "yargs": "^17.3.1"
  },
  "devDependencies": {
    "@types/buffers": "^0.1.33",
    "@types/express": "^4.17.20",
    "@types/json-schema": "^7.0.14",
    "@types/yargs": "^17.0.29",
    "browser-sync": "^2.29.3",
    "electron": "^27.0.2",
    "electron-builder": "24.6.*",
    "eslint": "^8.52.0",
    "istanbul": "^0.4.5",
    "license-checker": "^25.0.1",
    "mocha": "^10.2.0",
    "mocha-testdata": "^1.2.0",
    "sinon": "^17.0.0"
  },
  "main": "main.js",
  "author": "Shinichiro MORI",
  "license": "MIT"
}
